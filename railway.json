{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "java -Dspring.profiles.active=prod -jar /app/app.jar",
    "healthcheckPath": "/api/mobile/health",
    "healthcheckTimeout": 300
  }
}
